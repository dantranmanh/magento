<?xml version="1.0"?>
<config>
    <acl>
        <resources>
            <all>
                <title>Allow Everything</title>
            </all>
            <admin>
                <children>
                    <system>
                        <children>
                            <config>
                                <children>
                                    <kbase translate="title">
                                        <title>JET - Extensions Information</title>
                                    </kbase>
                                    <kstore translate="title">
                                        <title>JET - Extensions Store</title>
                                    </kstore>
                                </children>
                            </config>
                        </children>
                    </system>
                </children>
            </admin>
        </resources>
    </acl>
    <events>
        <controller_action_predispatch>
            <observers>
                <kbase_upds>
                    <type>singleton</type>
                    <class>kbase/feed</class>
                    <method>check</method>
                </kbase_upds>
            </observers>
        </controller_action_predispatch>
    </events>
</config>